<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
	xmlns:b.gwt="urn:import:org.gwtbootstrap3.client.ui.gwt" xmlns:b.html='urn:import:org.gwtbootstrap3.client.ui.html'>
	<b.gwt:HTMLPanel>
	    <b:Button ui:field="addTemplate"/>
		<b.gwt:HTMLPanel ui:field="templateList"/>

		<b:Modal ui:field="dialog" title="Select a data model for the new template:">
			<b:ModalBody>
				<b.gwt:FlowPanel>
					<b:LinkedGroup ui:field="dataModelList"/> 
				</b.gwt:FlowPanel>
			</b:ModalBody>
		</b:Modal>


	</b.gwt:HTMLPanel>

</ui:UiBinder>